(test
 (name lin_tests_dsl)
 (modules lin_tests_dsl)
 (package multicoretests)
 (flags (:standard -w -27))
 (libraries qcheck-lin.domain qcheck-lin.thread)
 (action (run %{test} --verbose))
)

